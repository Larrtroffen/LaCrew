# Environment variables for IntelliScrape Studio
# Copy this file to .env and fill in your actual API keys

# LLM API Keys (Uncomment and set the one you use)
# OPENAI_API_KEY="your_openai_api_key_here"
# ANTHROPIC_API_KEY="your_anthropic_api_key_here"
# GOOGLE_API_KEY="your_google_api_key_here"

# Search Engine API Keys (If using providers other than DuckDuckGo)
# GOOGLE_CSE_ID="your_google_custom_search_engine_id"
# GOOGLE_API_KEY="your_google_api_key_for_search"
# BING_SEARCH_V7_SUBSCRIPTION_KEY="your_bing_search_api_key"

# Optional: Override config path
# CONFIG_PATH="config/settings.custom.yaml" 